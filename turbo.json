{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "globalEnv": ["APOLLO_KEY", "APOLLO_GRAPH_REF", "OPENAI_API_KEY"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
